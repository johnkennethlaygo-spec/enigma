{
  "name": "enigma-agent",
  "version": "0.1.0",
  "private": true,
  "type": "module",
  "description": "Enigma trader productivity and risk assistant CLI",
  "main": "dist/server.js",
  "bin": {
    "enigma": "dist/cli.js"
  },
  "scripts": {
    "build": "tsc -p tsconfig.json && bash ./scripts/postbuild.sh",
    "dev": "tsx src/server.ts",
    "start": "node dist/server.js",
    "web": "tsx src/server.ts",
    "enigma": "tsx src/cli.ts",
    "lint": "eslint . --ext .ts",
    "test": "bash ./scripts/smoke-test.sh && npm run -s test:unit",
    "test:unit": "tsx --test tests/**/*.test.ts",
    "qa:extended": "bash ./scripts/extended-qa.sh",
    "qa": "npm run build && npm run test && npm run qa:extended"
  },
  "engines": {
    "node": ">=22"
  },
  "dependencies": {
    "@solana/web3.js": "^1.98.4",
    "bs58": "^6.0.0",
    "commander": "^12.1.0",
    "dotenv": "^16.4.5",
    "express": "^4.21.2",
    "express-rate-limit": "^7.5.0",
    "jsonwebtoken": "^9.0.2",
    "tweetnacl": "^1.0.3"
  },
  "devDependencies": {
    "@types/express": "^5.0.0",
    "@types/jsonwebtoken": "^9.0.9",
    "@types/node": "^22.10.1",
    "tsx": "^4.19.2",
    "typescript": "^5.7.2"
  }
}
